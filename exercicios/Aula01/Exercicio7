import java.util.Scanner;

public class LerIdadeESalario {
    public static void main(String[] args) {
        Scanner ler = new Scanner(System.in);
        System.out.println ("Digite o seu salário")
        int salarioMinimo = 2000
        int idadeMinima = 18
        if (salario <= salarioMinimo) {
            System.out.println ("Você tem renda suficiente")
        }
        else {
        System.out.println ("Você não tem renda suficiente") 
        }
        
        if (idade <= 18) {
            System.out.println ("Você tem idade suficiente")
        }
        else {
            System.out.println ("Você não tem idade suficiente")
        }
        scanner.close();
    }
}
    